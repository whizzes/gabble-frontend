{
  "name": "gabble-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "coverage": "vitest run --coverage",
    "dev": "vite dev",
    "dev:open": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config",
    "lint": "rome format ./src && rome check ./src",
    "format": "rome format --write ./src && rome check --apply ./src",
    "format:unsafe": "rome format --write ./src && rome check --apply-unsafe ./src",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@testing-library/svelte": "^4.0.3",
    "@urql/core": "^4.0.7",
    "@urql/exchange-auth": "^2.1.2",
    "@urql/svelte": "^4.0.1",
    "@whizzes/svelte-forms": "^0.2.0",
    "@whizzes/svelte-notifications": "^0.1.4",
    "@whizzes/exo": "file:../exo",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "rome": "^12.1.2",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "unplugin-icons": "^0.16.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "yup": "^1.2.0"
  },
  "type": "module"
}
